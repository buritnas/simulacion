<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.insurplay.app.insurplay">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_insurplay"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginRedesSociales" />
        <activity android:name=".Inicio" />
        <activity android:name=".Desafio" />
        <activity android:name=".CompetenciaSaludPersonal" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".SaludModoJuego"></activity>
    </application>

</manifest>